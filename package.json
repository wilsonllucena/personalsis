{
  "name": "windmill-dashboard-react",
  "version": "0.4.0",
  "description": "A multi theme, completely accessible, with components and pages examples, (almost) ready for production dashboard.",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts start",
    "prebuild": "npm run tailwind:build",
    "build": "react-scripts build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "tsc": "tsc"
  },
  "dependencies": {
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "^0.6.0",
    "axios": "^0.24.0",
    "chart.js": "2.9.3",
    "classnames": "^2.3.1",
    "faker": "4.1.0",
    "react": "^16.13.1",
    "react-chartjs-2": "2.9.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-hook-form": "^7.19.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.3",
    "react-transition-group": "4.4.1"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.3.1",
    "@types/react-router-dom": "^5.1.7",
    "autoprefixer": "^10.2.6",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "^5.0.6",
    "cz-conventional-changelog": "3.2.0",
    "postcss": "^8.3.2",
    "postcss-cli": "^8.3.1",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "^2.1.4",
    "typescript": "^4.3.2"
  },
  "keywords": [
    "windmill",
    "dashboard",
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
